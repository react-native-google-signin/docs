{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "startDG": "ENABLE_DOC_GEN=true docusaurus start",
    "start": "ENABLE_DOC_GEN=false docusaurus start",
    "build": "docusaurus build && node scripts/clean-llm-files.mjs",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "prettier:write": "prettier --write 'docs/**/*.{md,mdx}'",
    "prettier:check": "prettier --list-different 'docs/**/*.{md,mdx}'"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/plugin-ideal-image": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@mdx-js/react": "^3.1.1",
    "clsx": "^2.1.1",
    "docusaurus-lunr-search": "^3.6.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@docusaurus/faster": "^3.9.2",
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@docusaurus/tsconfig": "^3.9.2",
    "@docusaurus/types": "^3.9.2",
    "@types/react": "^19.2.7",
    "docusaurus-plugin-llms": "^0.2.2",
    "docusaurus-plugin-typedoc": "^1.4.2",
    "prettier": "^3.7.4",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.10.0",
    "typescript": "~5.9.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  },
  "packageManager": "yarn@4.9.2"
}
