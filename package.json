    "clsx": "^2.1.1",
    "docusaurus-lunr-search": "^3.6.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@docusaurus/faster": "^3.8.1",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@types/react": "^19.1.12",
    "docusaurus-plugin-typedoc": "^1.4.2",
    "prettier": "^3.6.2",
    "typedoc": "^0.28.11",
